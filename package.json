    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^1.1.4",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  },
  "license": "MIT"
